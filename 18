public class Driver 
{
	public static void main(String[] args)
	{
		String s = "hello";
		String s2 = "he11o";
		Driver.stringToCharArray(s2);
		System.out.println(Driver.stringToCharArray(s2));
		
		
	}
	
	static String stringToCharArray(String s)
	{
		String word = "";
		char[] array = new char[s.length()];
		 for (int i=0;i<s.length();i++){
		       array[i]=s.charAt(i);
		       word = word + s.charAt(i) + "";
		   }
		
		return word;
	}
	}
